{
  "name": "qa-test-pages",
  "version": "1.0.0",
  "description": "Test pages for manual QA",
  "scripts": {
    "lint": "./node_modules/standard/bin/cmd.js && ./node_modules/@linthtml/linthtml/bin/linthtml.js **/*.html *.html",
    "lint:js": "./node_modules/standard/bin/cmd.js",
    "lint:js:fix": "./node_modules/standard/bin/cmd.js --fix",
    "lint:html": "./node_modules/@linthtml/linthtml/bin/linthtml.js **/*.html *.html",
    "serve": "./node_modules/http-server/bin/http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brave-experiments/qa-test-pages.git"
  },
  "author": "Peter Snyder <pes@brave.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brave-experiments/qa-test-pages/issues"
  },
  "homepage": "https://github.com/brave-experiments/qa-test-pages#readme",
  "devDependencies": {
    "@linthtml/linthtml": "^0.4.2",
    "http-server": "^14.1.1",
    "standard": "^16.0.3"
  },
  "standard": {
    "ignore": [
      "static/js/libs/*",
      "fingerprinting/static/js/workers/fingerprinting-farbling.js",
      "static/js/service-workers/fingerprinting-farbling.js"
    ]
  },
  "dependencies": {
    "argparse": "^1.0.10"
  }
}
